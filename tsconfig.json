{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"strictNullChecks": true,
		"allowJs": true,
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@styles/*": ["src/styles/*"],
			"@layouts/*": ["src/layouts/*"],
			"@consts": ["src/consts.ts"],
			"@utils/*": ["src/util/*"]
		}
	}
}
